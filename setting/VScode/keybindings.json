[
  {
      "key": "ctrl+d",
      "command": "editor.action.deleteLines",
      "when": "editorTextFocus && !editorReadonly"
  },
  {
      "key": "ctrl+shift+k",
      "command": "-editor.action.deleteLines",
      "when": "editorTextFocus && !editorReadonly"
  },
  {
      "key": "ctrl+s",
      "command": "workbench.action.files.saveAll"
  },
  {
      "key": "ctrl+k s",
      "command": "-workbench.action.files.saveAll"
  },
  {
      "key": "ctrl+k ctrl+s",
      "command": "workbench.action.files.save"
  },
  {
      "key": "ctrl+s",
      "command": "-workbench.action.files.save"
  },
  {
      "key": "ctrl+shift+u",
      "command": "editor.action.transformToUppercase"
  },
  {
